, "components/Auth/Login/index.js", "pages/_app.js", "pages/auth/register.js", "pages/auth/login.js", "pages/dashboard.js", "pages/index.js", "pages/api/login.js", "pages/api/profile.js", "helpers/index.js", "helpers/const.js"  ],
  "exclude": [
